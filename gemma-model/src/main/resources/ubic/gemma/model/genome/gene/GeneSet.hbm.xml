<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.genome.gene.GeneSetImpl" table="GENE_SET" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <many-to-one name="auditTrail" class="ubic.gemma.model.common.auditAndSecurity.AuditTrailImpl"  cascade="all" lazy="proxy" fetch="select">
         <column name="AUDIT_TRAIL_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <many-to-one name="status" class="ubic.gemma.model.common.auditAndSecurity.StatusImpl"  cascade="all" lazy="proxy" fetch="select">
         <column name="STATUS_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <property name="name" type="java.lang.String">
         <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="description" type="ubic.gemma.persistence.hibernate.usertypes.HibernateStringClobType">
         <column name="DESCRIPTION" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci"/>
      </property>
      <set name="characteristics" lazy="false" fetch="select" inverse="false" cascade="all">
         <cache usage="read-write" />
         <key foreign-key="CHARACTERISTIC_GENE_SET_FKC">
            <column name="GENE_SET_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.common.description.CharacteristicImpl"/>
      </set>
      <many-to-one name="sourceAccession" class="ubic.gemma.model.common.description.DatabaseEntryImpl"  cascade="all" lazy="false" fetch="select">
         <column name="SOURCE_ACCESSION_FK" not-null="false" sql-type="BIGINT" unique="true"/>
      </many-to-one>
      <set name="literatureSources" table="GENE_SETS2LITERATURE_SOURCES" lazy="true" fetch="select" inverse="false">
         <key foreign-key="BIBLIOGRAPHIC_REFERENCE_GENE_SETS_FKC">
            <column name="GENE_SETS_FK" sql-type="BIGINT"/>
         </key>
         <many-to-many class="ubic.gemma.model.common.description.BibliographicReferenceImpl" >
            <column name="LITERATURE_SOURCES_FK" sql-type="BIGINT"/>
         </many-to-many>
      </set>
      <set name="members" lazy="true" fetch="select" inverse="false" cascade="all">
         <cache usage="read-write" />
         <key foreign-key="GENE_SET_MEMBER_GENE_SET_FKC">
            <column name="GENE_SET_FK" sql-type="BIGINT"/>
         </key>
         <one-to-many class="ubic.gemma.model.genome.gene.GeneSetMemberImpl"/>
      </set>
   </class>
</hibernate-mapping>