<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
	<class name="ubic.gemma.model.genome.ChromosomeFeature" table="CHROMOSOME_FEATURE" dynamic-insert="false"
		dynamic-update="false">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long" unsaved-value="null">
			<column name="ID" sql-type="BIGINT" />
			<generator class="native">
			</generator>
		</id>
		<discriminator column="class" type="string" />
		<property name="name" type="java.lang.String">
			<column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci" />
		</property>
		<property name="description" type="ubic.gemma.persistence.hibernate.usertypes.HibernateStringClobType">
			<column name="DESCRIPTION" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci" />
		</property>
		<property name="ncbiId" type="java.lang.String">
			<column name="NCBI_ID" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci" />
		</property>
		<property name="previousNcbiId" type="java.lang.String">
			<column name="PREVIOUS_NCBI_ID" not-null="false" unique="false"
				sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci" />
		</property>
		<many-to-one name="cytogenicLocation" class="ubic.gemma.model.genome.CytogeneticLocationImpl" cascade="all"
			lazy="proxy" fetch="select">
			<column name="CYTOGENIC_LOCATION_FK" not-null="false" sql-type="BIGINT" unique="true" />
		</many-to-one>
		<many-to-one name="physicalLocation" class="ubic.gemma.model.genome.PhysicalLocationImpl" cascade="all"
			lazy="proxy" fetch="select">
			<column name="PHYSICAL_LOCATION_FK" not-null="false" sql-type="BIGINT" unique="true" />
		</many-to-one>
		<subclass name="ubic.gemma.model.genome.gene.GeneProductImpl" discriminator-value="GeneProductImpl"
			dynamic-insert="false" dynamic-update="false" abstract="false">
			<property name="type" type="ubic.gemma.model.genome.gene.GeneProductTypeEnum">
				<column name="TYPE" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci" />
			</property>
			<property name="ncbiGi" type="java.lang.String">
				<column name="NCBI_GI" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci" />
			</property>
			<set name="accessions" lazy="true" fetch="select" inverse="false" cascade="all">
				<cache usage="read-write" />
				<key foreign-key="DATABASE_ENTRY_GENE_PRODUCT_FKC">
					<column name="GENE_PRODUCT_FK" sql-type="BIGINT" />
				</key>
				<one-to-many class="ubic.gemma.model.common.description.DatabaseEntryImpl" />
			</set>
			<set name="exons" lazy="true" fetch="select" inverse="false" cascade="all">
				<cache usage="read-write" />
				<key foreign-key="PHYSICAL_LOCATION_EXON_LOCATION_GENE_PRODUCT_FKC">
					<column name="EXON_LOCATION_GENE_PRODUCT_FK" sql-type="BIGINT" />
				</key>
				<one-to-many class="ubic.gemma.model.genome.PhysicalLocationImpl" />
			</set>
			<many-to-one name="gene" class="ubic.gemma.model.genome.GeneImpl" cascade="none" lazy="proxy" fetch="select">
				<column name="GENE_FK" not-null="false" sql-type="BIGINT" />
			</many-to-one>
		</subclass>
		<subclass name="ubic.gemma.model.genome.GeneImpl" discriminator-value="GeneImpl" dynamic-insert="false"
			dynamic-update="false" abstract="false">
			<property name="officialSymbol" type="java.lang.String">
				<column name="OFFICIAL_SYMBOL" not-null="false" unique="false"
					sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci" />
			</property>
			<property name="officialName" type="ubic.gemma.persistence.hibernate.usertypes.HibernateStringClobType">
				<column name="OFFICIAL_NAME" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci" />
			</property>
			<property name="ncbiGeneId" type="java.lang.Integer">
				<column name="NCBI_GENE_ID" not-null="false" unique="false" sql-type="INTEGER" />
			</property>
			<property name="ensemblId" type="java.lang.String">
				<column name="ENSEMBL_ID" not-null="false" unique="false"
					sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci" />
			</property>
			<set name="products" lazy="true" fetch="select" inverse="true" cascade="all">
				<cache usage="read-write" />
				<key foreign-key="GENE_PRODUCT_GENE_FKC">
					<column name="GENE_FK" sql-type="BIGINT" />
				</key>
				<one-to-many class="ubic.gemma.model.genome.gene.GeneProductImpl" />
			</set>
			<set name="aliases" lazy="true" fetch="select" inverse="false" cascade="all-delete-orphan">
				<cache usage="read-write" />
				<key foreign-key="GENE_ALIAS_GENE_FKC">
					<column name="GENE_FK" sql-type="BIGINT" />
				</key>
				<one-to-many class="ubic.gemma.model.genome.gene.GeneAliasImpl" />
			</set>
			<many-to-one name="taxon" class="ubic.gemma.model.genome.TaxonImpl" lazy="false" fetch="select">
				<column name="TAXON_FK" not-null="false" sql-type="BIGINT" />
			</many-to-one>
			<set name="accessions" lazy="true" fetch="select" inverse="false" cascade="all">
				<cache usage="read-write" />
				<key foreign-key="DATABASE_ENTRY_GENE_FKC">
					<column name="GENE_FK" sql-type="BIGINT" />
				</key>
				<one-to-many class="ubic.gemma.model.common.description.DatabaseEntryImpl" />
			</set>
			<set name="citations" table="GENE_CITATIONS" lazy="true" fetch="select" inverse="false">
				<key foreign-key="BIBLIOGRAPHIC_REFERENCE_GENES_FKC">
					<column name="GENES_FK" sql-type="BIGINT" />
				</key>
				<many-to-many class="ubic.gemma.model.common.description.BibliographicReferenceImpl">
					<column name="CITATIONS_FK" sql-type="BIGINT" />
				</many-to-many>
			</set>
			<many-to-one name="multifunctionality" class="ubic.gemma.model.genome.gene.MultifunctionalityImpl"
				cascade="all" lazy="proxy" fetch="select">
				<column name="MULTIFUNCTIONALITY_FK" not-null="false" sql-type="BIGINT" unique="true" />
			</many-to-one>
			<set name="phenotypeAssociations" lazy="true" fetch="select" inverse="true" cascade="all">
				<cache usage="read-write" />
				<key foreign-key="PHENOTYPE_ASSOCIATION_GENE_FKC">
					<column name="GENE_FK" sql-type="BIGINT" />
				</key>
				<one-to-many class="ubic.gemma.model.association.phenotype.PhenotypeAssociation" />
			</set>
		</subclass>
	</class>
</hibernate-mapping>