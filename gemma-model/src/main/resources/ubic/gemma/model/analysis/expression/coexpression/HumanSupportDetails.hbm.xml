<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
	<class name="ubic.gemma.model.analysis.expression.coexpression.HumanCoexpressionSupportDetailsImpl" table="HUMAN_LINK_SUPPORT_DETAILS"
		dynamic-insert="false" dynamic-update="false"> 
		<id access="field" name="id" type="java.lang.Long" unsaved-value="null">
			<column name="ID" sql-type="BIGINT" />
			<generator class="org.hibernate.id.MultipleHiLoPerTableGenerator" />
		</id>

		<property name="bytes" type="byte[]">
			<column name="BYTES" not-null="true" unique="false" sql-type="MEDIUMBLOB" />
		</property>
	</class>

</hibernate-mapping>