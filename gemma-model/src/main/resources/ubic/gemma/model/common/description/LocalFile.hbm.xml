<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.common.description.LocalFileImpl" table="LOCAL_FILE" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <property name="localURL" type="ubic.gemma.persistence.hibernate.usertypes.HibernateURLType">
         <column name="LOCAL_U_R_L" not-null="false" unique="true" sql-type="VARCHAR(512)"/>
      </property>
      <property name="remoteURL" type="ubic.gemma.persistence.hibernate.usertypes.HibernateURLType">
         <column name="REMOTE_U_R_L" not-null="false" unique="false" sql-type="VARCHAR(512)"/>
      </property>
      <property name="version" type="java.lang.String">
         <column name="VERSION" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="size" type="java.lang.Long">
         <column name="SIZE" not-null="false" unique="false" sql-type="BIGINT"/>
      </property>
      <many-to-one name="format" class="ubic.gemma.model.common.description.FileFormatImpl"  lazy="proxy" fetch="select">
         <column name="FORMAT_FK" not-null="false" sql-type="BIGINT"/>
      </many-to-one>
      <set name="sourceFiles" table="SOURCE_FILES" lazy="true" fetch="select" inverse="false">
         <key foreign-key="LOCAL_FILE_LOCAL_FILES_FKC">
            <column name="LOCAL_FILES_FK" sql-type="BIGINT"/>
         </key>
         <many-to-many class="ubic.gemma.model.common.description.LocalFileImpl" >
            <column name="SOURCE_FILES_FK" sql-type="BIGINT"/>
         </many-to-many>
      </set>
   </class>
</hibernate-mapping>