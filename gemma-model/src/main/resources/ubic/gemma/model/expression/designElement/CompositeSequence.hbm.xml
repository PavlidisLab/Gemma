<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.expression.designElement.CompositeSequenceImpl" table="COMPOSITE_SEQUENCE" dynamic-insert="false" dynamic-update="false">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <property name="name" type="java.lang.String">
         <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <property name="description" type="ubic.gemma.persistence.hibernate.usertypes.HibernateStringClobType">
         <column name="DESCRIPTION" not-null="false" unique="false" sql-type="text character set latin1 collate latin1_swedish_ci"/>
      </property>
      <many-to-one name="biologicalCharacteristic" class="ubic.gemma.model.genome.biosequence.BioSequenceImpl"  lazy="proxy" fetch="select">
         <column name="BIOLOGICAL_CHARACTERISTIC_FK" not-null="false" sql-type="BIGINT"/>
      </many-to-one>
      <many-to-one name="arrayDesign" class="ubic.gemma.model.expression.arrayDesign.ArrayDesignImpl" cascade="none"  lazy="false" fetch="select">
         <column name="ARRAY_DESIGN_FK" not-null="true" sql-type="BIGINT"/>
      </many-to-one>
   </class>
</hibernate-mapping>