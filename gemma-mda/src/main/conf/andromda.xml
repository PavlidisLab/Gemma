<?xml version="1.0" encoding="UTF-8"?>
<!--  $Id$ -->
<andromda>
	<properties>
		<property name="modelValidation">${modelValidation}</property>
		<property name="cartridgeFilter">${filter}</property>
		<property name="loggingConfigurationUri">file:${project.basedir}/src/main/conf/log4j.xml</property>
	</properties>
	<server>
		<host>localhost</host>
		<port>4445</port>
	</server>
	<repositories>
		<repository name="netBeansMDR">
			<models>
				<model>
					<uri>jar:file:${project.basedir}/src/main/uml/gemma-om.xml.zip!/gemma-om.xml</uri>
					<moduleSearchLocations>
						<location patterns="**/*.xml.zip, **/*.uml14">
							${settings.localRepository}/org/andromda/profiles
						</location>
					</moduleSearchLocations>
				</model>
			</models>
		</repository>
	</repositories>
	<mappingsSearchLocations>
		<location patterns="*Mappings.xml">${project.basedir}/src/main/conf/</location>
	</mappingsSearchLocations>
	<namespaces>
		<namespace name="default">
			<properties>
				<property name="languageMappingsUri">Java</property>
				<property name="wrapperMappingsUri">JavaWrapper</property>
				<property name="sqlMappingsUri">file:${project.basedir}/src/main/conf/MySQLMappings.xml</property>
				<property name="jdbcMappingsUri">JDBC</property>
				<property name="maxSqlNameLength">60</property>
				<property name="hibernateVersion">3</property>
				<property name="enableTemplating">true</property>
			</properties>
		</namespace>
		<namespace name="java">
			<properties>
				<property name="enumerations">${maven.andromda.core.generated.dir}</property>
				<property name="exceptions">${maven.andromda.core.generated.dir}</property>
				<property name="value-objects">${maven.andromda.core.generated.dir}</property>
				<property name="enableTemplating">true</property>
			</properties>
		</namespace>
		<namespace name="bpm4struts">
			<!--  Note that we are not using this -->
			<properties>
				<property name="forms">${maven.andromda.core.generated.dir}</property>
				<property name="pages">${maven.andromda.web.generated.pages.dir}/pages</property>
				<property name="actions">${maven.andromda.core.generated.dir}</property>
				<property name="controllers">${maven.andromda.core.generated.dir}</property>
				<property name="controller-impls">${maven.andromda.core.manual.dir}</property>
				<property name="decorators">${maven.andromda.core.manual.dir}</property>
				<property name="messages">${maven.andromda.web.messages.dir}</property>
				<property name="configuration">${maven.andromda.web.conf.generated.dir}/WEB-INF</property>
				<property name="securityRealm"></property>
				<property name="serviceAccessorPattern">
					${application.package}.ServiceLocator.instance().get{1}()
				</property>
				<property name="manageableServiceAccessorPattern">
					${application.package}.spring.ManageableServiceLocator.instance().get{1}()
				</property>
			</properties>
		</namespace>
		<namespace name="spring">
			<properties>
				<property name="hibernateDialect">org.hibernate.dialect.MySQLMyISAMDialect</property>
				<property name="hibernateShowSql">false</property>
				<property name="hibernateUseQueryCache">true</property>
				<property name="hibernateJdbcBatchSize">500</property>
				<property name="hibernateJdbcFetchSize">500</property>
				<property name="hibernateMaxFetchDepth">3</property>
				<property name="hibernateDefaultBatchFetchSize">500</property>
				<property name="hibernateJdbcBatchVersionedData">true</property>
				<property name="hibernateJdbcUseStreamsForBinary">true</property>
				<property name="hibernateQueryUseNamedParameters">true</property>
				<property name="hibernateEnableCache">true</property>
				<property name="hibernateQueryCacheFactory">org.hibernate.cache.StandardQueryCacheFactory</property>
				<property name="hibernateCacheProvider">org.hibernate.cache.EhCacheProvider</property>
				<property name="hibernateInheritanceStrategy">subclass</property>
				<property name="enableTemplating">false</property>
				<property name="enableDaoPropertiesCreateMethod">false</property>
				<property name="springTypesPackage">ubic.gemma.spring</property>
				<property name="springPresentationTypesPackage">ubic.gemma.spring.presentation</property>
				<property name="mergeLocation">${maven.andromda.spring.merge.custom}</property>
				<property name="mergeMappingsUri">file:${maven.andromda.merge.mappings}</property>
				<property name="driver">${gemma.db.driver}</property>
				<property name="username">${gemma.db.user}</property>
				<property name="password">${gemma.db.password}</property>
				<property name="connectionUrl">${gemma.db.url}</property>
				<property name="serviceLocator">${maven.andromda.spring.service.locator.dir}</property>
				<property name="applicationContext">applicationContext-hibernate.xml</property>
				<property name="applicationContextLocalDataSource">dataSource.xml</property>
				<property name="jdbcDataSourceClass">org.apache.commons.dbcp.BasicDataSource</property>
				<property name="jdbcDataSourceDestroyMethod">close</property>
				<property name="localTransactionManager">
					org.springframework.orm.hibernate3.HibernateTransactionManager
				</property>
				<property name="sessionFactory">org.springframework.orm.hibernate3.LocalSessionFactoryBean</property>
				<property name="value-objects">${maven.andromda.core.generated.dir}</property>
				<property name="daos">${maven.andromda.core.generated.dir}</property>
				<property name="dao-impls">${maven.andromda.core.manual.dir}</property>
				<property name="services">${maven.andromda.core.generated.dir}</property>
				<property name="service-impls">${maven.andromda.core.manual.dir}</property>
				<property name="service-interfaces">${maven.andromda.core.generated.dir}</property>
				<property name="spring-configuration">${maven.andromda.spring.configuration.dir}</property>
				<property name="parameterRequiredCheck">false</property>
			</properties>
		</namespace>
		<namespace name="hibernate">
			<properties>
				<property name="hibernateDialect">org.hibernate.dialect.MySQLMyISAMDialect</property>
				<property name="hibernateTypeMappingsUri">
					file:${project.basedir}/src/main/conf/HibernateTypeMappings.xml
				</property>
				<property name="enableTemplating">true</property>
				<property name="hibernateQueryCacheFactory">org.hibernate.cache.StandardQueryCacheFactory</property>
				<property name="hibernateInheritanceStrategy">${hibernate.inheritance.strategy}</property>
				<property name="hibernateCacheProvider">org.hibernate.cache.EhCacheProvider</property>
				<property name="entities">${maven.andromda.core.generated.dir}</property>
				<property name="entity-impls">${maven.andromda.core.manual.dir}</property>
				<property name="entity-mappings">${maven.andromda.hibernate.configuration.dir}</property>
				<property name="user-types">${maven.andromda.core.generated.dir}</property>
				<property name="customTypesPackage">${application.package}</property>
				<property name="userTypesPackage">${application.package}.persistence.hibernate.usertypes</property>
				<property name="compositionDefinesEagerLoading">true</property>
				<property name="mergeLocation">${project.basedir}/src/main/custom/hibernate</property>
				<property name="mergeMappingsUri">file:${maven.andromda.merge.mappings}</property>
				<property name="hibernateCompositionCascade">all</property>
				<property name="hibernateAggregationCascade">none</property>
				<property name="hibernateDefaultCascade">none</property>
				<property name="specificCollectionInterfaces">false</property>
				<property name="associationEndCollectionIndexName">orderedCollectionIndex</property>
				<property name="configuration">${maven.andromda.hibernate.configuration.dir}</property>
				<property name="hibernateEnableCache">true</property>
				<property name="cache">${maven.andromda.hibernate.configuration.dir}</property>
				<property name="hibernateEnableAssociationsCache">true</property>
				<property name="ehCacheTimeToIdleSeconds">200</property>
				<property name="ehCacheTimeToLiveSeconds">200</property>
				<property name="ehCacheMaxElementsInMemory">2000</property>
				<property name="ehCacheOverflowToDisk">false</property>
				<property name="hibernateTransactionFactoryClass">
					org.hibernate.transaction.JTATransactionFactory
				</property>
				<!-- 				<property name="hibernateXMLPersistence">true</property>-->
			</properties>
		</namespace>
		<namespace name="cs">
			<properties>
				<property name="interfaces">${maven.andromda.core.generated.dir}</property>
			</properties>
		</namespace>
	</namespaces>
</andromda>
