<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
   <class name="ubic.gemma.model.genome.ChromosomeImpl" table="CHROMOSOME" dynamic-insert="false" dynamic-update="false" mutable="false">
      <cache usage="read-write" />
      <id access="field" name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <property access="field"  name="name" type="java.lang.String">
         <column name="NAME" not-null="true" unique="false" sql-type="VARCHAR(255) character set latin1 collate latin1_swedish_ci"/>
      </property>
      <many-to-one  access="field" name="assemblyDatabase" class="ubic.gemma.model.common.description.ExternalDatabaseImpl"  lazy="proxy" fetch="select">
         <column name="ASSEMBLY_DATABASE_FK" not-null="false" sql-type="BIGINT"/>
      </many-to-one>
      <many-to-one  access="field" name="sequence" class="ubic.gemma.model.genome.biosequence.BioSequenceImpl"  lazy="proxy" fetch="select">
         <column name="SEQUENCE_FK" not-null="false" sql-type="BIGINT"/>
      </many-to-one>
      <many-to-one  access="field" name="taxon" class="ubic.gemma.model.genome.TaxonImpl"  lazy="proxy" fetch="select">
         <column name="TAXON_FK" not-null="true" sql-type="BIGINT"/>
      </many-to-one>
   </class>
</hibernate-mapping>