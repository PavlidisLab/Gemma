<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://hibernate.org/xsd/hibernate-mapping-4.0.xsd">

   <class name="ubic.gemma.model.association.phenotype.PhenotypeAssociationPublication" table="PHENOTYPE_ASSOCIATION_PUBLICATIONS">
      <cache usage="read-write" />
      <id name="id" type="java.lang.Long" unsaved-value="null">
         <column name="ID" sql-type="BIGINT"/>
         <generator class="native">
         </generator>
      </id>
      <many-to-one name="citation" class="ubic.gemma.model.common.description.BibliographicReference" lazy="proxy"
				fetch="select">
				<column name="CITATION_FK" not-null="false" sql-type="BIGINT" />
	  </many-to-one>
	  <property name="type" type="java.lang.String">
         <column name="TYPE" not-null="false" unique="false" sql-type="VARCHAR(255)"/>
      </property>
   </class>
</hibernate-mapping>